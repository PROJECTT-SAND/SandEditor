{
  "name": "sand-editer",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.js",
  "productName": "SAND-editer",
  "homepage": "./",
  "Authors": [
    "lavi",
    "dacoon"
  ],
  "Description": "Game engine for coding beginner",
  "dependencies": {
    "@monaco-editor/react": "^4.4.5",
    "@react-three/drei": "^8.11.0",
    "@react-three/fiber": "^7.0.26",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@use-gesture/react": "^10.2.7",
    "electron": "^18.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.6",
    "electron-squirrel-startup": "^1.0.0",
    "node-gyp": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "react-spring": "npm:@react-spring/web@^9.0.0-rc.1",
    "three": "^0.138.0",
    "web-vitals": "^2.1.4",
    "zustand": "^3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build_r": "set PUBLIC_URL=.&& react-scripts build",
    "build_e": "electron-forge make",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron": "^17.2.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "sand_editer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "build": {
    "appId": "com.sand.sandediter",
    "productName": "SAND-editer",
    "nsis": {
      "oncClick": false,
      "installerIcon": "icon.ico",
      "createDesktopShortcut": true
    }
  }
}
